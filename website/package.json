{
	"name": "teeline.online",
	"version": "0.0.1",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "npm run format && npm run lint && npm run test && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check --plugin prettier-plugin-svelte .",
		"format": "prettier --write --plugin prettier-plugin-svelte .",
		"test": "vitest run",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^2.0.2",
		"@storybook/addon-essentials": "^8.3.2",
		"@storybook/addon-interactions": "^8.3.2",
		"@storybook/addon-links": "^8.3.2",
		"@storybook/blocks": "^8.3.2",
		"@storybook/svelte": "^8.3.2",
		"@storybook/svelte-vite": "^8.3.2",
		"@storybook/sveltekit": "^8.3.2",
		"@storybook/test": "^8.3.2",
		"@sveltejs/adapter-netlify": "^4.3.4",
		"@sveltejs/kit": "^2.5.28",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tsconfig/svelte": "^5.0.4",
		"@typescript-eslint/eslint-plugin": "7.16.1",
		"@typescript-eslint/parser": "7.16.1",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-storybook": "^0.8.0",
		"eslint-plugin-svelte": "^2.44.0",
		"mdsvex": "^0.12.3",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"storybook": "^8.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^3.8.6",
		"svelte-preprocess": "^6.0.2",
		"typescript": "~5.5.4",
		"vitest": "^2.1.1"
	},
	"type": "module",
	"dependencies": {
		"vite": "^5.4.7"
	}
}